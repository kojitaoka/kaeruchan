<table class="table table-striped">

  <h2>item detail</h2>
  <thead>
    <tr>
	    <th>Title</th>
	    <th></th>
		</tr>
	</thead>

	<tbody>
		<% @items.each do |item| %>
			<tr>
						<td><%= item.title %></td>
						<td><%= @disc.disc_number %></td>
						<td><%= @song.name  %></td>
						<% end %>
			</tr>
		<% end %>
		
	</tbody>
</table>
					



